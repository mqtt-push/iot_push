<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lxr.iot.plugin.mysql.mapper.WillMsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lxr.iot.plugin.mysql.entity.WillMsg">
    <result column="id" property="id" />
    <result column="update_time" property="updateTime" />
        <result column="willTopic" property="willTopic" />
        <result column="willMessage" property="willMessage" />
        <result column="qos" property="qos" />
        <result column="retain" property="retain" />
        <result column="creeate_time" property="creeateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    id,
    update_time AS updateTime,
        willTopic, willMessage, qos, retain, creeate_time AS creeateTime
    </sql>

</mapper>
