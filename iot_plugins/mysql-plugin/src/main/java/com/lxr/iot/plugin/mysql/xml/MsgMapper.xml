<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lxr.iot.plugin.mysql.mapper.MsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lxr.iot.plugin.mysql.entity.Msg">
    <result column="id" property="id" />
    <result column="create_time" property="createTime" />
    <result column="update_time" property="updateTime" />
        <result column="msgid" property="msgid" />
        <result column="topic" property="topic" />
        <result column="sender" property="sender" />
        <result column="node" property="node" />
        <result column="qos" property="qos" />
        <result column="retain" property="retain" />
        <result column="payload" property="payload" />
        <result column="arrived" property="arrived" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
    id,
    create_time AS createTime,
    update_time AS updateTime,
        msgid, topic, sender, node, qos, retain, payload, arrived
    </sql>

</mapper>
